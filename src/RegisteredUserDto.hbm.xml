<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dto.RegisteredUserDto" table="users"
		schema="goodbye">


		<id name="id">
			<column name="id" sql-type="int(255)"></column>
		</id>
		<property name="firstName">
			<column name="firstName" sql-type="varchar(255)"></column>
		</property>
		
		<property name="lastName">
			<column name="lastName" sql-type="varchar(255)"></column>
			</property>
			
		<property name="address1">
			<column name="address1" sql-type="varchar(255)"></column>
		</property>
		
		<property name="address2">
			<column name="address2" sql-type="varchar(255)"></column>
		</property>
		
		<property name="city">
			<column name="city" sql-type="varchar(255)"></column>
		</property>
		
		<property name="state">
			<column name="state" sql-type="varchar(255)"></column>
		</property>
		
		<property name="zip">
			<column name="zip" sql-type="int(9)"></column>
		</property>
		
		<property name="country">
			<column name="country" sql-type="varchar(2)"></column>
		</property>
		
		<property name="date">
			<column name="date" sql-type="varchar(255)"></column>
		</property>
		
		
	</class>
</hibernate-mapping>	